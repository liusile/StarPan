<Page x:Class="StarPan.SubPage.TransferPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:StarPan.SubPage"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="TransferPage">
    <DockPanel>
        <Border BorderBrush="{StaticResource Border.Split}" BorderThickness="1" Width="150" DockPanel.Dock="Left">
            <ListBox x:Name="nemuBox" DockPanel.Dock="Top" Background="#F9FAFB" BorderThickness="0"  Width="208" Style="{StaticResource LeftTree}">
                <ListBox.ItemContainerStyle >
                    <Style TargetType="ListBoxItem">
                        <Setter Property="BorderThickness" Value="0"></Setter>
                        <Setter Property="Padding" Value="0"></Setter>
                    </Style>
                </ListBox.ItemContainerStyle>
            </ListBox>
        </Border>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" >
            <Image Source="../image/downLarge.png" Width="128"></Image>
            <TextBlock FontSize="15" Margin="0 30">当前没有下载任务喔~</TextBlock>
        </StackPanel>
    </DockPanel>
</Page>
